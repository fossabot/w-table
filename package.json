{
  "name": "w-table",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook -s www"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "0.10.0-11",
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "@storybook/addon-actions": "v4.0.0-alpha.11",
    "@storybook/addon-info": "v4.0.0-alpha.11",
    "@storybook/addon-knobs": "v4.0.0-alpha.11",
    "@storybook/addon-viewport": "v4.0.0-alpha.11",
    "@storybook/html": "^4.0.0-alpha.11",
    "@types/jest": "23.1.4",
    "babel-core": "6.26.3",
    "babel-runtime": "6.26.0",
    "jest": "23.3.0",
    "react": "16.4.1",
    "react-dom": "16.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-component-starter.git"
  },
  "author": "hinterdupfinger",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
